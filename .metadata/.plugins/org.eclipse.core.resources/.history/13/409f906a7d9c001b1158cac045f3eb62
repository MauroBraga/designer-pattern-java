package br.com.mrb.loja.desconto;

import java.math.BigDecimal;

import br.com.mrb.loja.orcamento.Orcamento;

public class CalculadoraDeDesconto {
	
	public BigDecimal calcular(Orcamento orcamento) {
		BigDecimal desconto =  new DescontoParaOrcamentoComMaisDeCincoItens(
				new DescontoParaOrcamentoComValorMaiorQueQuinhentos(proximo).calcular(orcamento)).calcular(orcamento);
		
		
		return desconto;
	}

}
